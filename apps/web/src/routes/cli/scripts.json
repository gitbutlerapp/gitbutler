					"<span class='t-success'>✓</span> <span class='t-teal'>Committed</span> <span class='t-accent'>abc123d</span> on branch <span class='t-highlight'>feature/api-base</span>",
					""
				]
			},
			{
				"type": "input",
				"lines": ["$ but branch new feature/api-auth"]
			},
			{
				"type": "output",
				"lines": [
					"<span class='t-success'>✓</span> <span class='t-teal'>Created branch</span> <span class='t-highlight'>feature/api-auth</span> from <span class='t-dim'>main</span>",
					""
				]
			},
			{
				"type": "input",
				"lines": ["$ but push"]
			},
			{
				"type": "output",
				"lines": [
					"<span class='t-success'>✓</span> <span class='t-teal'>Pushed</span> <span class='t-highlight'>2</span> branches:",
					"→ <span class='t-cyan'>feature/api-base</span>",
					"→ <span class='t-cyan'>feature/api-auth</span>"
				]
			}
		]
	},
