	"simple-rebasing": {
		"id": "simple-rebasing",
		"title": "Simple Editing",
		"icon": "<svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 20 20\"><path fill=\"currentColor\" d=\"M10.562 1.915c1.368-1.171 3.4.24 2.78 1.932l-1.43 3.903 4.39.002c1.587 0 2.355 1.943 1.197 3.027l-7.504 7.028c-1.33 1.245-3.44-.118-2.852-1.843l1.505-4.411h-4.61c-1.625 0-2.372-2.023-1.138-3.08l7.662-6.558Zm1.128 1.074a.234.234 0 0 0-.153.066L3.876 9.612a.25.25 0 0 0 .162.44h6.708l-.339.993-1.845 5.403a.236.236 0 0 0-.004.168.268.268 0 0 0 .105.123c.054.035.11.049.156.046a.235.235 0 0 0 .15-.073l7.505-7.028a.25.25 0 0 0-.171-.432l-5.465-.002-1.073-.001.369-1.008 1.8-4.91a.234.234 0 0 0 .008-.166.268.268 0 0 0-.1-.126.266.266 0 0 0-.152-.05Z\"/></svg>",
