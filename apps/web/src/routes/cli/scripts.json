					"",
					"  (<span class='t-dim'>last commit in the primary lane</span>)",
					"    bookmarks.ts <span class='t-dim'>+1,1</span>",
					"    config.ru <span class='t-dim'>@4,3 +4,4</span>",
					""
				]
			},
			{
				"type": "input",
				"lines": ["$ but amend app/models/user.rb da42d06"]
			},
			{
				"type": "output",
				"lines": [
					"<span class='t-success'>✓</span> Amended <span class='t-highlight'>app/models/user.rb</span> → <span class='t-accent'>da42d06</span>",
					""
				]
			},
			{
				"type": "input",
				"lines": ["$ but squash da42d06 fdbd753"]
			},
			{
				"type": "output",
				"lines": [
					"<span class='t-success'>✓</span> Squashed <span class='t-accent'>da42d06</span> → <span class='t-accent'>fdbd753</span>",
					""
				]
			},
			{
				"type": "input",
				"lines": ["$ but move 21 feature-branch"]
			},
			{
				"type": "output",
				"lines": [
					"<span class='t-success'>✓</span> Moved commit <span class='t-accent'>21de2f</span> → <span class='t-highlight'>[feature-branch]</span>"
				]
			}
		]
	},
