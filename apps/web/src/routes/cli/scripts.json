{
	"stacked-branches": {
		"id": "stacked-branches",
		"title": "Stacked Branches",
		"icon": "<svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 21 20\"><path fill=\"currentColor\" d=\"M17.081 2.004A3 3 0 0 1 19.927 5v10.233l-.004.156a3 3 0 0 1-2.842 2.84l-.154.004H4.078l-.154-.003a3 3 0 0 1-2.846-2.997V5a3 3 0 0 1 2.846-2.996L4.078 2h12.849l.154.004ZM2.578 15.12v.113a1.5 1.5 0 0 0 1.5 1.5h12.849a1.5 1.5 0 0 0 1.5-1.5v-.113H2.578Zm0-4.639v3.14h15.849v-3.14H2.578Zm1.5-6.981a1.5 1.5 0 0 0-1.5 1.5v3.981h15.849V5a1.5 1.5 0 0 0-1.5-1.5H4.078Z\"/></svg>",
		"script": [
			{
				"type": "input",
				"lines": ["$ but branch new user-bookmarks"]
			},
			{
				"type": "output",
				"lines": [
					"<span class='t-success'>✓</span> <span class='t-teal'>Created branch</span> <span class='t-highlight'>user-bookmarks</span>",
					""
				]
			},
			{
				"type": "input",
				"lines": ["$ but commit -m 'Add user bookmarks feature'"]
			},
			{
				"type": "output",
				"lines": [
					"<span class='t-success'>✓</span> <span class='t-teal'>Created commit</span> <span class='t-accent'>e677a2e</span> on branch <span class='t-highlight'>user-bookmarks</span>",
					""
				]
			},
			{
				"type": "input",
				"lines": ["$ but branch new -a user-bookmarks liked-tweets"]
			},
			{
				"type": "output",
				"lines": [
					"<span class='t-success'>✓</span> <span class='t-teal'>Created branch</span> <span class='t-highlight'>liked-tweets</span> stacked on <span class='t-dim'>user-bookmarks</span>",
					""
				]
			},
			{
				"type": "input",
				"lines": ["$ but commit -m 'Add liked tweets feature' liked-tweets"]
			},
			{
				"type": "output",
				"lines": [
					"<span class='t-success'>✓</span> <span class='t-teal'>Created commit</span> <span class='t-accent'>71f4fe2</span> on branch <span class='t-highlight'>liked-tweets</span>",
					""
				]
			},
			{
				"type": "input",
				"lines": ["$ but status"]
			},
			{
				"type": "output",
				"delay": 1500,
				"lines": [
					"╭┄<span class='t-blue'>zz</span> [<span class='t-green'>unstaged changes</span>]</span>",
					"┊     <span class='t-dim'>no changes</span>",
					"┊",
					"┊╭┄<span class='t-blue'>li</span> [<span class='t-green'>liked-tweets</span>]",
					"┊●   <span class='t-blue'>e8</span><span class='t-dim'>a47da</span> Add liked tweets feature",
					"┊│",
					"┊├┄<span class='t-blue'>us</span> [<span class='t-green'>user-bookmarks</span>]",
					"┊●   <span class='t-blue'>d4</span><span class='t-dim'>c5a30</span> Add user bookmarks feature",
					"├╯",
					"┊",
					"┴ <span class='t-dim'>96ccca9</span> (common base) [<span class='t-green'>origin/main</span>] <span class='t-dim'>2026-01-26</span> Point release"
				]
			}
		]
	},
	"parallel-branches": {
