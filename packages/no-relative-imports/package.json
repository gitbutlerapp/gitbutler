{
	"name": "@gitbutler/no-relative-imports",
	"version": "0.0.3",
	"license": "MIT",
	"private": false,
	"description": "An ESLint rule for non-relative imports in TypeScript",
	"homepage": "https://github.com/gitbutlerapp/gitbutler/tree/master/packages/no-relative-imports",
	"keywords": [
		"typescript",
		"eslint",
		"imports",
		"relative",
		"absolute"
	],
	"type": "module",
	"peerDependencies": {
		"eslint": ">=9.0.0"
	},
	"devDependencies": {
		"typescript": "catalog:",
		"vitest": "catalog:"
	},
	"scripts": {
		"package": "tsc",
		"test": "vitest run",
		"typecheck:tests": "bash typecheck-tests.sh",
		"prepare": "pnpm package"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"dependencies": {
		"comment-json": "^4.2.5"
	}
}
